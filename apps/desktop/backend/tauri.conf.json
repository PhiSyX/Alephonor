{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "Alephonor",
	"mainBinaryName": "Alephonor",
	"version": "0.1.0",
	"identifier": "com.phisyx.alephonor",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "Alephonor",
				"minWidth": 800,
				"minHeight": 600,
				"devtools": true,
				"contentProtected": true,
				"hiddenTitle": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": []
	}
}
